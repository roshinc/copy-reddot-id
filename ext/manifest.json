{
  "name": "Copy RedDot ID",
  "description": "Copys the ID of the current RedDot page",
  "version": "0.0.2",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Copy ID!"
  },
  "icons": {
    "128": "icon/icon.png",
    "16": "icon/icon.png",
    "48": "icon/icon.png"
  },
  "manifest_version": 2
}
